/* pages/index/car.wxss */
page{
    background: #f5f5f5;
}
.main{
    background: #fff;
}
.main_list1 .item-title{
    color: #3c3c3c;
    font-size:14px; 
    display:flex;
    margin-top: 10rpx;
}
.item-title1{
    flex:1;
    text-align: center;
}
.info{
    margin-top: 20rpx;
    background: #fff;
    padding: 20rpx 10px;
    display: flex;
}
.img{
    width: 50px;
    height: 50px;
    vertical-align: middle;

}
.info-name{
    font-size: 14px;
    margin-left: 10px;
}
.star{
    width: 15px;
    height: 15px;
    display: inline-block
}
.star image{
    width: 10px;
    height: 10px;
}
.text{
    font-size: 12px;
    color:#666;
}
.miao{
    background: #fff;
    margin-top: 20rpx;
    padding: 10px;
}
.title{
    font-size: 14px;
    color:#3c3c3c;
    border-bottom: 1px solid #ddd;
    padding-bottom: 20rpx;
}
.ul{
    margin-top: 10px;
    font-size: 13px;
    color:#666;
}
.address-info{
    font-size: 14px;
    margin-top: 10px;
}
.btn{
    overflow: hidden;
    background: #fff;
    position: fixed;
    left:0;
    bottom: 0;
    width: 100%;
    right:0;
    text-align: center;
    display:flex;
    padding-top: 10px;
}
button{
    width: 46%;
    display: inline-block;

}
button[type="default"]{
    border:1px solid green;
    color:green;
}